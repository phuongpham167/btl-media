<div>
  <% @rooms.each do |room| %>
    <div class="person">
        <div class="col-xs-2 avatar">
            <img alt="avatar" src="/assets/anh-1.png" style="width: 40px; height :40px; " />
        </div>
        <div class="col-xs-10 nameperson">
            <span><a href="/party/<%= room.id %>"><%= room.name %></a></span>
            <span class="state"></span>
        </div>
    </div>
  <% end %>
</div>
